{
  "name": "Dimmed Dark Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.constant": {
      "foreground": "#ca4e4e"
    },
    "variable.defaultLibrary": {
      "foreground": "#bbbbbb"
    }
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#bbbbbbff",
        "background": "#121212ff"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "string.comment"],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#bfb058"
      }
    },
    {
      "name": "js template-expression",
      "scope": "punctuation.definition.template-expression.begin.js,punctuation.definition.template-expression.end.js,punctuation.definition.template-expression.begin.ts,punctuation.definition.template-expression.end.ts",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "TemplateString",
      "scope": "string.template.js",
      "settings": {
        "foreground": "#bfb058"
      }
    },
    {
      "name": "TemplateString",
      "scope": "meta.template.expression.js",
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Embeded String Begin and End",
      "scope": ["string.embedded.begin", "string.embedded.end"],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "name": "Embeded String",
      "scope": "string.embedded",
      "settings": {
        "foreground": "#8758bf"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#Bf587c"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.readwrite", "variable.readwrite.other.block"],
      "settings": {
        "foreground": "#96be25"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
