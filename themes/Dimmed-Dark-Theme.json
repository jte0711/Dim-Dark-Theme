      "settings": {
        "foreground": "#Bf587c"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#58a5bf"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#96be25"
      }
    },
    {
      "name": "Variable Object",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#96be25"
      }
    },
    {
      "name": "Other variable",
